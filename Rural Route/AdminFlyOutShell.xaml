<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Rural_Route.AdminFlyOutShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Rural_Route"
    Shell.FlyoutBehavior="Flyout"
    BackgroundColor="White"
    >

    <ShellContent
        Title="Create Product"
        ContentTemplate="{DataTemplate local:AdminCreateProduct}"
        Route="AdminCreateProduct" />

    <ShellContent
    Title="Create Order"
    ContentTemplate="{DataTemplate local:adminCreateDelivery}"
    Route="adminCreateDelivery" />

    <ShellContent
    Title="Create Customer"
    ContentTemplate="{DataTemplate local:AdminCreateCustomer}"
    Route="AdminCreateCustomer" />
    
    <ShellContent
    Title="Create Employee"
    ContentTemplate="{DataTemplate local:Admin}"
    Route="Admin" />

    <ShellContent
        Title="Logout"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    
</Shell>
