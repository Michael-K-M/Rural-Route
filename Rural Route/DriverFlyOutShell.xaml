<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Rural_Route.DriverFlyOutShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Rural_Route"
    Shell.FlyoutBehavior="Flyout"
    BackgroundColor="White">

    <ShellContent
        Title="DriverHomepage"
        ContentTemplate="{DataTemplate local:DriverHomepage}"
        Route="DriverHomepage" />


    <ShellContent
        Title="SalesRepHomepage"
        ContentTemplate="{DataTemplate local:SalesRepHomepage}"
        Route="SalesRepHomepage" />


    <ShellContent
        Title="Admin"
        ContentTemplate="{DataTemplate local:Admin}"
        Route="Admin" />
</Shell>
