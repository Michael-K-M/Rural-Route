<?xml version="1.0" encoding="UTF-8" ?>
<Shell 
    x:Class="Rural_Route.SaleRepFlyOutShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Rural_Route"
    Shell.FlyoutBehavior="Flyout"
    BackgroundColor="DarkOliveGreen">

    <ShellContent
    Title="Homepage"
    ContentTemplate="{DataTemplate local:SalesRepHomepage}"
    Route="SalesRepHomepage" />

    <ShellContent
Title="Stock"
ContentTemplate="{DataTemplate local:SaleRepStock}"
Route="SaleRepStock" />
  
    <ShellContent
Title="Sale"
ContentTemplate="{DataTemplate local:SaleRepSaleCount}"
Route="SaleRepSaleCount" />

    <ShellContent
    Title="Logout"
    ContentTemplate="{DataTemplate local:MainPage}"
    Route="MainPage" />

</Shell>